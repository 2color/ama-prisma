{
  "name": "prisma-ama",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "generate": "PRISMA_CLIENT_ENGINE_TYPE='dataproxy' prisma generate",
    "build": "npm run prod:db:push && PRISMA_CLIENT_ENGINE_TYPE='dataproxy' prisma generate && next build",
    "start": "next start",
    "lint": "eslint ./src/pages/* ./src/components/* ./src/pages/api/* ./src/lib/* ./src/types/* -c .eslintrc.json --fix --ext ts,tsx",
    "prepare": "husky install",
    "db:push": "prisma db push",
    "prod:db:push": "dotenv -- sh -c 'PRISMA_CLIENT_ENGINE_TYPE=\"dataproxy\" DATABASE_URL=\"$MIGRATE_DATABASE_URL\" prisma db push'",
    "prod:db:seed": "dotenv -- sh -c 'PRISMA_CLIENT_ENGINE_TYPE=\"dataproxy\" DATABASE_URL=\"$MIGRATE_DATABASE_URL\" prisma db seed'"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "0.5.2-next.15",
    "@prisma/client": "3.3.0-integration-feat-data-proxy-client.25",
    "@tailwindcss/forms": "^0.3.4",
    "@tailwindcss/typography": "^0.4.1",
    "@tippy.js/react": "^3.1.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "babel-plugin-superjson-next": "^0.4.1",
    "cloudinary": "^1.27.1",
    "cookie": "^0.4.1",
    "graphql": "^15.6.1",
    "next": "https://next-middleware-build.vercel.sh/next-v12.0.0-nightly.25.tgz",
    "next-auth": "4.0.0-next.26",
    "next-seo": "^4.28.1",
    "prismjs": "^1.25.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-feather": "^2.0.9",
    "react-hot-toast": "^2.1.1",
    "react-linkify": "^1.0.0-alpha",
    "react-markdown": "^7.0.1",
    "react-query": "^3.26.0",
    "string-replace-to-array": "^2.1.0",
    "styled-components": "^5.3.1",
    "superjson": "^1.7.5",
    "timeago.js": "^4.0.2"
  },
  "devDependencies": {
    "@types/node": "^16.10.3",
    "@types/react": "^17.0.27",
    "@typescript-eslint/parser": "^4.33.0",
    "autoprefixer": "^10.3.7",
    "babel-plugin-styled-components": "^1.13.2",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-mdx": "^1.15.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.3",
    "postcss": "^8.3.8",
    "postcss-import": "^14.0.2",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.4.1",
    "prettier-eslint": "^13.0.0",
    "prisma": "3.3.0-integration-feat-data-proxy-client.25",
    "stylelint-config-recommended": "^5.0.0",
    "tailwindcss": "^2.2.16",
    "typescript": "^4.4.3",
    "typescript-eslint-parser": "^22.0.0",
    "webpack": "^5.58.1"
  },
  "lint-staged": {
    "*.+(ts|tsx)": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "pre-commit": "lint:staged && generate",
  "license": "MIT"
}
